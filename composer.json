{
    "name": "gene/module-braintree",
    "description": "Fork from the Magento Braintree 2.2.0 module by Gene Commerce for PayPal.",
    "version": "3.0.5",
    "require": {
        "magento/module-config": "101.0.*",
        "magento/module-directory": "100.2.*",
        "magento/module-payment": "100.2.*",
        "magento/module-checkout": "100.2.*",
        "magento/module-sales": "101.0.*",
        "magento/module-backend": "100.2.*",
        "magento/module-vault": "101.0.*",
        "magento/module-customer": "101.0.*",
        "magento/module-catalog": "102.0.*",
        "magento/module-quote": "101.0.*",
        "magento/module-paypal": "100.2.*",
        "magento/module-ui": "101.0.*"
    },
    "suggest": {
        "magento/module-checkout-agreements": "100.2.*",
        "magento/module-theme": "100.2.*"
    },
    "replace": {
	    "magento/module-braintree": "100.2.*"
    },
    "type": "magento2-module",
    "license": [
        "proprietary"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Magento\\Braintree\\": ""
        }
    },
    "authors": [
        {
            "name": "Magento Commerce",
            "homepage": "http://www.magento.com",
            "role": "Original Developers"
        },
        {
            "name": "Aidan Threadgold from Gene Commerce",
            "homepage": "http://www.gene.co.uk",
            "role": "Developer"
        }
    ]
}
